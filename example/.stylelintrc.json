{
  "plugins": [
    "../lib/index"  
  ],
  "rules": {
    "sustainable-css/avoid-duplicate-media-queries": true,
    "sustainable-css/concatenate-property-values": true,
    "sustainable-css/no-important-tags": true,
    "sustainable-css/no-semicolons": true,
    "sustainable-css/no-vendor-prefixes": true,
    "sustainable-css/optimise-selectors": true,
    "sustainable-css/remove-redundancies": true,
    "sustainable-css/shorten-properties": true,
    "sustainable-css/shorten-units": true,
    "sustainable-css/use-special-characters": true
  }
}
